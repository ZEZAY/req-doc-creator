<h4>Projects</h4>
<br>
<%= link_to new_project_path do %><%= button_tag 'Add', class: 'btn btn-primary my-1' %><% end %>

<br><br>
<% @projects.each do |project| %>
  <%= link_to project_path(project) do %><%= button_tag 'view', class: 'btn btn-secondary my-1' %><% end %>
  <%= link_to edit_project_path(project) do %><%= button_tag 'edit', class: 'btn btn-secondary my-1' %><% end %>
  <%= project.name %>
  <br>
<% end %>